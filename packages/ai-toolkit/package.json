{
  "name": "@crm-immobiliare/ai-toolkit",
  "version": "1.0.0",
  "description": "AI agents and tools for CRM Immobiliare - TypeScript/Datapizza wrapper",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@crm-immobiliare/shared-types": "workspace:*",
    "@google/generative-ai": "^0.17.1",
    "axios": "^1.6.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@crm-immobiliare/config": "workspace:*",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "ai",
    "agents",
    "tools",
    "crm",
    "real-estate",
    "gemini",
    "datapizza"
  ],
  "author": "CRM Immobiliare Team",
  "license": "PRIVATE"
}
